show ip config = ip addr show
add an ipaddress = ip addr add
delete an ipaddress = ip addr del
enable interface = ip link set eth1 up
disable network interface = ip link set eth1 down
show route table = ip route show
add a static route =  ip route add 10.10.10.0/24 via 192.168.1.10 dev eth0
delete a static route = ip route del 10.10.10.0/24
add default gateway =  ip route add default via 192.168.1.10
